/GitHub/Lito_Portfolio/src/components/featuredProject.js",["143"],[],"import React from \"react\"\r\nimport styled from \"@emotion/styled\"\r\nimport { theme, media } from '../styles'\r\nimport TechList from './TechList';\r\nimport Image from './image'\r\n\r\nconst FeaturedProjectContainer = styled.div`\r\n  display: flex;\r\n  flex-direction: column;\r\n  padding: 20px;\r\n  border-radius: ${theme.borderRadius};\r\n  background-color: ${theme.colors.light};\r\n  box-shadow: ${theme.boxShadow};\r\n\r\n  &.featured {\r\n    h5 {\r\n      font-size: 30px;\r\n      line-height: 38px;\r\n    }\r\n  }\r\n`\r\n\r\nconst ImgContainer = styled.div`\r\n  width: 80%;\r\n  margin-bottom: 20px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n\r\n  ${media.small} {\r\n    max-width: 100%;\r\n  }\r\n`\r\n\r\nconst Title = styled.h3`\r\n  max-width: 100%;\r\n  text-align: left;\r\n  margin-bottom: 10px;\r\n  color: ${theme.colors.darkLight};\r\n`\r\n\r\nconst Description = styled.p`\r\n  color: ${theme.colors.darkLight};\r\n  margin-bottom: 15px;\r\n`\r\n\r\nconst FlexRow = styled.div`\r\n  margin-top: auto;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n\r\n  ${media.small} {\r\n    flex-direction: column-reverse;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n  }\r\n`\r\n\r\nconst ProjectLinks = styled.div`\r\n  justify-content: flex-end;\r\n  display: flex;\r\n\r\n  ${media.small} {\r\n    width: 100%;\r\n    margin-bottom: 15px;\r\n    justify-content: center;\r\n  }\r\n`\r\n\r\nconst ProjectLink = styled.a`\r\n  padding: 12px 20px;\r\n  color: ${theme.colors.darkLight};\r\n  background-color:${theme.colors.yellow};\r\n  box-shadow: ${theme.boxShadow};\r\n\r\n  border-radius: ${theme.borderRadius};\r\n  white-space: nowrap;\r\n  transition: ${theme.transition};\r\n\r\n  :nth-of-type(2) {\r\n    margin-left: 10px;\r\n  }\r\n\r\n  &:hover {\r\n    box-shadow: ${theme.boxShadowHover};\r\n  }\r\n\r\n  ${media.small} {\r\n    width: 50%;\r\n    margin-left: 0;\r\n    text-align: center;\r\n    flex-grow: 1;\r\n  }\r\n`\r\n \r\nconst FeaturedProject = React.forwardRef(({ img,soon, title, classes, description, techList, siteLink, codeLink }, ref) => {\r\n\r\n  return (\r\n    <FeaturedProjectContainer ref={ref} className={classes}>\r\n        <ImgContainer>\r\n        </ImgContainer>\r\n        <Title>{title}</Title>\r\n        <Description>\r\n          {description}\r\n        </Description>\r\n        <FlexRow>\r\n          <TechList items={techList} soon={soon} />\r\n          <ProjectLinks>\r\n              { siteLink  ?\r\n               ( <ProjectLink href={siteLink} target='_blank'>Visit Site ›</ProjectLink>) : null\r\n              }\r\n              { codeLink  ?\r\n                (<ProjectLink href={codeLink} target='_blank'>View Code ›</ProjectLink>) : null\r\n              }\r\n          </ProjectLinks>\r\n        </FlexRow>\r\n    </FeaturedProjectContainer>\r\n  )\r\n})\r\n\r\nexport default FeaturedProject\r\n\r\n","/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/styles/index.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/components/Navigation.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/components/TechList.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/components/image.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/styles/GlobalStyles.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/styles/container.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/styles/theme.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/styles/Section.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/styles/media.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/components/socialIconLinks.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Portfolio/src/components/images/instagram.js",[],[],"/Users/litobanano/Documents/GitHub/Lito_Port